{
    "name": "augmint-contracts",
    "version": "0.1.0",
    "description": "Augmint Stable Tokens - Solidity contracts",
    "homepage": ".",
    "private": true,
    "scripts": {
        "start":
            "./runganache.sh & $(yarn bin)/truffle migrate --reset && echo 'Migration done. Contracts deployed to ganache. Contract artifacts are in build/contracts folder.' & wait",
        "build": "truffle compile",
        "clean": "rm build/contracts/*",
        "test": "truffle test",
        "coverage": "solidity-coverage",
        "migrate": "truffle migrate",
        "compile": "yarn build",
        "ganache:run": "./runganache.sh",
        "runmigrate": "yarn start"
    },
    "devDependencies": {
        "babel-register": "6.26.0",
        "bignumber.js": "5.0.0",
        "coveralls": "3.0.0",
        "ganache-cli": "6.1.0",
        "moment": "2.21.0",
        "random-seed": "0.3.0",
        "solidity-coverage": "0.4.14",
        "stringifier": "1.3.0",
        "truffle": "4.1.3"
    },
    "greenkeeper": {
        "ignore": ["bignumber.js"]
    }
}
