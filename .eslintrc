{
    "extends": [
        "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 2017
    },
    "env": {
        "mocha": true,
        "node": true,
        "es6": true
    },
    "rules": {
        "indent": "off",
        "max-len": "off",
        "radix": "off",
        "no-unused-vars": "warn",
        "no-console": "off"
    }
}